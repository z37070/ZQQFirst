                app:layout_constraintGuide_percent="0.135" />

            <TextView
                android:id="@+id/textview_main_mine2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textview_main_mine2_value"
                android:textSize="19sp"
                app:layout_constraintBottom_toTopOf="@+id/guideline55"
                app:layout_constraintStart_toStartOf="@+id/guideline58"
                app:layout_constraintTop_toTopOf="@+id/guideline54" />

            <TextView
                android:id="@+id/textview_main_mine5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textview_main_mine5_value"
                android:textSize="19sp"
